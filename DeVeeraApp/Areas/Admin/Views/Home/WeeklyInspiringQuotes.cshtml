@model IList<DeVeeraApp.ViewModels.DashboardQuoteModel>

@{
    ViewData["Title"] = "Weekly Inspiring Quotes";

}

<div class="intro-y flex flex-col sm:flex-row items-center mt-8">
    <h2 class="text-lg font-medium mr-auto">
        Weekly Inspiring Quotes
    </h2>
</div>
@if (Model.Count != 0)
{
<div class="intro-y mt-10 col-span-12 lg:col-span-8">
    <div class="mt-5 pt-5 border-t border-theme-25">
        @foreach (var item in Model)
        {
            <div class="alert alert-primary show mb-2" role="alert">
                <div class="flex items-center">
                    <div class="font-medium text-lg">@item.Author</div>

                </div>
                <div class="mt-3">
                    @item.Title
                </div>
            </div>
        }
    </div>

    </div>


    }
    else
    {
    <h2 class="text-lg font-medium mr-auto">
        There are no new Videos yet.
    </h2>
    }

