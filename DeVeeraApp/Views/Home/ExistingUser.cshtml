@model DeVeeraApp.ViewModels.WeeklyUpdateModel

@{
    ViewData["Title"] = "ExistingUser";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" class="light">
<head>
    <meta charset="utf-8">
    <link href="dist/images/logo.svg" rel="shortcut icon">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Midone admin is super flexible, powerful, clean & modern responsive tailwind admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, Midone admin template, dashboard template, flat admin template, responsive admin template, web app">
    <meta name="author" content="LEFT4CODE">
    <title>Existing User</title>
    <link rel="stylesheet" href="~/dist/css/app.css" />
</head>

<body class="main">
    <div class="container">
        <div class="error-page flex flex-col lg:flex-row items-center justify-center h-screen text-center lg:text-left">
            <div class="-intro-x lg:mr-20">
                @{string src = Model.VideoURL;}
                <video src="@Url.Content(src)" controls style="width:100%; height:400px" loop />
            </div>
            <div class="text-white mt-10 lg:mt-0">
                <div class="intro-x text-8xl font-medium">@Model.Title </div>
                <div class="intro-x text-xl lg:text-3xl font-medium mt-5">@Model.Subtitle </div>
                <a asp-controller="Lesson" asp-action="Index" asp-route-id="0" asp-route-isNewUser="false" class="intro-x btn py-3 px-4 text-white border-white dark:border-dark-5 dark:text-gray-300 mt-10">Go To Dashboard</a>
            </div>
        </div>
    </div>
    <script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBG7gNHAhDzgYmq4-EHvM4bqW1DNj2UCuk&amp;libraries=places"></script>
    <script src="dist/js/app.js"></script>

</body>
</html>
