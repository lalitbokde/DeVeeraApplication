@model DeVeeraApp.ViewModels.WeeklyUpdateModel
@{
    ViewData["Title"] = "NewUser";
    Layout = null;
}

<!doctype html>
<html lang="zxx">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/bootstrap.min.css">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/animate.min.css">
    <!-- Meanmenu CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/meanmenu.css">
    <!-- Boxicons CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/boxicons.min.css">
    <!-- Flaticon CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/flaticon.css">
    <!-- Odometer CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/odometer.min.css">
    <!-- Slick Min CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/slick.min.css">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/nice-select.min.css">
    <!-- Carousel CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/owl.carousel.min.css">
    <!-- Carousel Default CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/owl.theme.default.min.css">
    <!-- Magnific Popup CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/magnific-popup.min.css">
    <!-- Fancybox CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/fancybox.min.css">
    <!-- Style CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/style.css">
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/responsive.css">

    <title>Psylo - Life Coach & Psychologist HTML Template</title>

    <link rel="icon" type="image/png" href="~/UserAssets/images/favicon.png">
</head>


<body>

    <!-- Start Preloader Area -->
    <div class="preloader-area">
        <div class="spinner">
            <div class="inner">
                <div class="disc"></div>
                <div class="disc"></div>
                <div class="disc"></div>
            </div>
        </div>
    </div>
    <!-- End Preloader Area -->
    <!-- Start Main Banner Area -->
    <div class="main-banner-area">
        <div class="main-banner-item-box">
            <div class="d-table">
                <div class="d-table-cell">
                    <div class="container-fluid">
                        <div class="row align-items-center">
                            <div class="col-lg-6">
                                <div class="main-banner-image">
                                    <img src="~/UserAssets/images/main-banner/banner-1.jpg" alt="image">

                                    <div class="image-shape">
                                        <img src="~/UserAssets/images/main-banner/shape.png" alt="image">
                                    </div>
                                </div>

                            </div>

                            <div class="col-lg-6">
                                <div class="main-banner-content">
                                    <h1> @Model.Title <span>Psychologist</span></h1>
                                    @Html.Raw(@Model.Subtitle)
                                    <div class="banner-btn">
                                        <a asp-controller="Home" asp-action="AskHappynessLevel" class="default-btn">@T("Lets Start") <i class="flaticon-pointer"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="main-banner-shape">
            <img src="~/UserAssets/images/main-banner/banner-shape-1.png" alt="image">
        </div>

        <div class="main-banner-shape-two">
            <img src="~/UserAssets/images/main-banner/banner-shape-2.png" alt="image">
        </div>
    </div>
    <!-- End Main Banner Area -->
    <!-- Start About Area -->
    <section class="about-area bg-ffffff ptb-100">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="about-main-content">
                        <h3>Know About Us</h3>

                        <div class="about-content-image">
                            <img src="~/UserAssets/images/about/about-2.jpg" alt="image">

                            <h4 class="sub-title">Happe</h4>

                            <a href="@Model.VideoUrl" class="video-btn popup-youtube">
                                <i class="flaticon-play"></i>
                            </a>
                        </div>
                     
                    

                        <div class="about-btn">
                            <a asp-controller="Diary" asp-action="AskUserEmotion" class="default-btn">@T("Lets Start") <i class="flaticon-user"></i></a>
                        </div>
                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="about-main-image">
                        <img src="~/UserAssets/images/about/about-1.jpg" alt="image">

                        <div class="about-shape about-wrap">
                            <div class="shape-1">
                                <img src="~/UserAssets/images/about/shape-1.png" alt="image">
                            </div>

                            <div class="shape-2">
                                <img src="~/UserAssets/images/about/shape-2.png" alt="image">
                            </div>

                            <div class="shape-3">
                                <img src="~/UserAssets/images/about/shape-3.png" alt="image">
                            </div>

                            <div class="shape-4">
                                <img src="~/UserAssets/images/about/shape-4.png" alt="image">
                            </div>

                            <div class="shape-5">
                                <img src="~/UserAssets/images/about/shape-5.png" alt="image">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End About Area -->
    <!-- Start Go Top Area -->
    <div class="go-top">
        <i class='flaticon-up-arrow'></i>
    </div>
    <!-- End Go Top Area -->
    <!-- Jquery Slim JS -->
    <script src="~/UserAssets/js/jquery.min.js"></script>
    <!-- Bootstrap Bundle JS -->
    <script src="~/UserAssets/js/bootstrap.bundle.min.js"></script>
    <!-- Meanmenu JS -->
    <script src="~/UserAssets/js/jquery.meanmenu.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/UserAssets/js/owl.carousel.min.js"></script>
    <!-- Jquery Appear JS -->
    <script src="~/UserAssets/js/jquery.appear.js"></script>
    <!-- Odometer JS -->
    <script src="~/UserAssets/js/odometer.min.js"></script>
    <!-- Slick Min JS -->
    <script src="~/UserAssets/js/slick.min.js"></script>
    <!-- Nice Select JS -->
    <script src="~/UserAssets/js/nice-select.min.js"></script>
    <!-- Magnific Popup JS -->
    <script src="~/UserAssets/js/jquery.magnific-popup.min.js"></script>
    <!-- Fancybox JS -->
    <script src="~/UserAssets/js/fancybox.min.js"></script>
    <!-- Ajaxchimp JS -->
    <script src="~/UserAssets/js/jquery.ajaxchimp.min.js"></script>
    <!-- Form Validator JS -->
    <script src="~/UserAssets/js/form-validator.min.js"></script>
    <!-- Contact JS -->
    <script src="~/UserAssets/js/contact-form-script.js"></script>
    <!-- Wow JS -->
    <script src="~/UserAssets/js/wow.min.js"></script>
    <!-- Custom JS -->
    <script src="~/UserAssets/js/main.js"></script>
</body>
</html>


