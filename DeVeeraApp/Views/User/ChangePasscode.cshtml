@model DeVeeraApp.ViewModels.UserLogin.DiaryPasscodeModel
@{
    ViewData["Title"] = "ChangePasscode";
    Layout = null;
}

<!doctype html>
<html lang="zxx">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/bootstrap.min.css">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/animate.min.css">
    <!-- Meanmenu CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/meanmenu.css">
    <!-- Boxicons CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/boxicons.min.css">
    <!-- Flaticon CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/flaticon.css">
    <!-- Odometer CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/odometer.min.css">
    <!-- Slick Min CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/slick.min.css">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/nice-select.min.css">
    <!-- Carousel CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/owl.carousel.min.css">
    <!-- Carousel Default CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/owl.theme.default.min.css">
    <!-- Magnific Popup CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/magnific-popup.min.css">
    <!-- Fancybox CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/fancybox.min.css">
    <!-- Style CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/style.css">
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="~/UserAssets/css/responsive.css">

    <link href="~/dist/css/Rating.css" rel="stylesheet" />


    <title>Psylo - Life Coach & Psychologist HTML Template</title>

    <link rel="icon" type="image/png" href="~/UserAssets/images/favicon.png">
</head>

<body>

    <!-- Start Preloader Area -->
    <div class="preloader-area">
        <div class="spinner">
            <div class="inner">
                <div class="disc"></div>
                <div class="disc"></div>
                <div class="disc"></div>
            </div>
        </div>
    </div>
    <!-- End Preloader Area -->
    <!-- Start Coming Soon Area -->

    <div class="coming-soon-area" style="background-color:lightgray">
        <div class="d-table">
            <div class="d-table-cell">
                <div class="coming-soon-content">
                    <h1>
                        <a href="#">Change Diary Passcode</a>
                    </h1>

                    <h2>Change your passcode.</h2>



                    <section class="login-area mt-5">
                        <div class="container">
                            <div class="login-form">
                                <form asp-controller="User" asp-action="ChangePasscode" method="post">
                                    <div class="form-group">
                                        <input asp-for="OldPassword" type="number" class="form-control" placeholder="old-passcode">
                                        <span asp-validation-for="OldPassword" class="text-theme-6 mt-2"></span>
                                    </div>
                                    <div class="form-group">
                                        <input asp-for="Password" type="number" class="form-control" placeholder="passcode">
                                        <span asp-validation-for="Password" class="text-theme-6 mt-2"></span>

                                    </div>
                                    <button type="submit">Submit</button>
                                    <button type="submit">Continue to Diary</button>
                                </form>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </div>

    <!-- End Coming Soon Area -->
    <!-- Jquery Slim JS -->
    <script src="~/UserAssets/js/jquery.min.js"></script>
    <!-- Bootstrap Bundle JS -->
    <script src="~/UserAssets/js/bootstrap.bundle.min.js"></script>
    <!-- Meanmenu JS -->
    <script src="~/UserAssets/js/jquery.meanmenu.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/UserAssets/js/owl.carousel.min.js"></script>
    <!-- Jquery Appear JS -->
    <script src="~/UserAssets/js/jquery.appear.js"></script>
    <!-- Odometer JS -->
    <script src="~/UserAssets/js/odometer.min.js"></script>
    <!-- Slick Min JS -->
    <script src="~/UserAssets/js/slick.min.js"></script>
    <!-- Nice Select JS -->
    <script src="~/UserAssets/js/nice-select.min.js"></script>
    <!-- Magnific Popup JS -->
    <script src="~/UserAssets/js/jquery.magnific-popup.min.js"></script>
    <!-- Fancybox JS -->
    <script src="~/UserAssets/js/fancybox.min.js"></script>
    <!-- Ajaxchimp JS -->
    <script src="~/UserAssets/js/jquery.ajaxchimp.min.js"></script>
    <!-- Form Validator JS -->
    <script src="~/UserAssets/js/form-validator.min.js"></script>
    <!-- Contact JS -->
    <script src="~/UserAssets/js/contact-form-script.js"></script>
    <!-- Wow JS -->
    <script src="~/UserAssets/js/wow.min.js"></script>
    <!-- Custom JS -->
    <script src="~/UserAssets/js/main.js"></script>

    <script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBG7gNHAhDzgYmq4-EHvM4bqW1DNj2UCuk&amp;libraries=places"></script>
</body>
</html>



